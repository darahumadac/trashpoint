{
  "expo": {
    "name": "basic-react-native-app",
    "slug": "basic-react-native-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/trashpoint_logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icons/trashpoint_logo.png",
      "resizeMode": "center",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/trashpoint_logo.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": ["./assets/fonts"]
        }
      ]
    ]
  }
}
